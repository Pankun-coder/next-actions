{
  "family": "NEXTJS-Actions",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "next-actions",
      "image": "next-actions",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "secrets": [
        {
          "name": "NEXT_PUBLIC_TEST_ENV",
          "valueFrom": "my-secret"
        }
      ]
    }
  ],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "requiresCompatibilities": ["FARGATE"],
  "taskRoleArn": "arn:aws:iam::312001397697:role/taskRole",
  "executionRoleArn": "ecsTaskExecutionRole",
  "cpu": "256",
  "memory": "512"
}
